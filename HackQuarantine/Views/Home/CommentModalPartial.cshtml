@model HackQuarantine.Models.CommentRequest
<div class="modal fade" id="storeModal" role="dialog">
    <div class="modal-dialog">

        <!-- Modal content-->
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title">Add a Comment</h4>
                <button type="button" class="close" data-dismiss="modal">&times;</button>
            </div>
            <div class="modal-body">
                <form id="commentForm" asp-action="CommentRequest" method="post">
                    <div asp-validation-summary="All"></div>
                    <input type="hidden" asp-for="StoreId" id="storeId" />
                    <input type="hidden" asp-for="ItemId" id="itemId" />
                    <div class="form-group">
                        <label asp-for="Price">Price</label>
                        <input class="form-control" asp-for="Price" />
                    </div>
                    <div class="form-group">
                        <label asp-for="SaleStatus">Sale Status?</label>
                        <select class="form-control" asp-for="SaleStatus">
                            <option value="" selected>--Choose here--</option>
                            <option value="LowQuantity">Low Quantity</option>
                            <option value="MediumQuantity">Medium Quantity</option>
                            <option value="HighQuantity">High Quantity</option>
                        </select>
                    </div>
                    <div class="form-group">
                        <label asp-for="InStock">In Stock?</label>
                        <select class="form-control" asp-for="InStock">
                            <option value="" selected>--Choose here--</option>
                            <option value="true">Yes</option>
                            <option value="false">No</option>
                        </select>
                    </div>
                    <div class="form-group">
                        <label asp-for="Notes">Notes</label>
                        <textarea class="form-control" asp-for="Notes"></textarea>
                    </div>
                    <button type="submit" class="btn btn-primary" data-save="modal">Submit</button>
                </form>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
            </div>
        </div>

    </div>
</div>
<script>
    alert('here');
</script>